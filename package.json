{
  "name": "nextjs-ts-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build:dev": "cross-env APP_ENV=development next build",
    "build:qa": "cross-env APP_ENV=qa next build",
    "build:uat": "cross-env APP_ENV=uat next build",
    "build:prod": "cross-env APP_ENV=production next build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "format": "eslint src --fix"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "next": "12.1.5",
    "next-routes": "^1.4.2",
    "polished": "^4.2.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-error-overlay": "6.0.11",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.27",
    "@types/react": "18.0.7",
    "@types/react-dom": "18.0.0",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^28.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-preset-react-app": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.11.0",
    "eslint-config-next": "^0.2.4",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "jest": "^28.0.1",
    "jest-environment-jsdom": "^28.0.1",
    "prettier": "^2.1.1",
    "typescript": "4.6.3"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
